{
    "name": "nixphp/app",
    "description": "Starter app for your project when using NixPHP",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Flo Knapp",
            "email": "office@florianknapp.de"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../nixphp"
        },
        {
            "type": "path",
            "url": "../nixphp-form"
        }
    ],
    "require": {
        "php": ">=8.3",
        "nixphp/framework": "dev-main",
        "nixphp/form": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "dev"
}
